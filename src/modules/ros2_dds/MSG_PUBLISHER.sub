			<Connection Source="PUBLISH_1.INITO" Destination="INITO" Comment="" dx1="695.0"/>
			<Connection Source="REQ" Destination="PUBLISH_1.REQ" Comment="" dx1="1695.0"/>
		</EventConnections>
		<DataConnections>
